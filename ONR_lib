//请在下面写上您的子程序或函数
//写完保存后，在任一命令库上点击右键并选择“刷新”即可
//
Function report_open(search_str)
    //通用 - 打开Report界面
    MoveTo 591, 456
    Delay 300
    KeyDown "LeftAlt", 1
    KeyDown "Alt", 1
    Delay 30
    KeyDown "LeftAlt", 1
    KeyDown "Alt", 1
    Delay 20
    KeyDown "M", 1
    Delay 100
    KeyUp "M", 1
    Delay 300
    KeyUp "LeftAlt", 1
    KeyUp "Alt", 1
    Delay 300
    KeyDown "R", 1
    Delay 90
    KeyUp "R", 1
	
    //SayString 处输入搜索报表字段
    Delay 200
    SayString search_str
    Delay 200
	
    //通用 - 操作为：搜索报表，勾选Print to PDF，打开报表选项
    MoveTo 583, 340
    MoveTo 785, 231
    Delay 200
    LeftClick 1
    MoveTo 487, 589
    Delay 200
    LeftClick 1
    MoveTo 605, 333
    Delay 200
    LeftClick 1
    Delay 50
    LeftClick 1
    Delay 200
End Function


Function report_save(savename)
    //SayString 处输入需要保存的文件名
    Delay 500
    SayString savename
    Delay 200
	
    //通用 - 操作为：保存，等待8秒后关闭reports界面
    KeyDown "Enter", 1
    Delay 100
    KeyUp "Enter", 1
	
    Delay 5000
	
    MoveTo 600, 600
    LeftClick 1
    Delay 200
    KeyDown "Alt", 1
    KeyDown "LeftAlt", 1
    Delay 70
    KeyDown "C", 1
    Delay 90
    KeyUp "LeftAlt", 1
    KeyUp "Alt", 1
    Delay 10
    KeyUp "C", 1
End Function


Function Comp()
    LockMouse

    Call report_open("%complimentary")
	
    //专用 - 操作为：报表相关选项选择
    KeyDown "LeftAlt", 1
    KeyDown "Alt", 1
    Delay 440
    KeyDown "F", 1
    Delay 80
    KeyUp "F", 1
    Delay 20
    KeyUp "LeftAlt", 1
    KeyUp "Alt", 1
    Delay 300
    KeyDown "BackSpace", 1
    Delay 83
    KeyUp "BackSpace", 1
	
    Call report_save("comp.pdf")
    
    UnlockMouse
End Function


