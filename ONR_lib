Function RS05this()
'##########日期计算##########
//夜审前日期确定（非必须）
my_month = Month(Date-1)
If my_month < 10 Then 
	my_month =  "0" & my_month
End If

my_day = Day(Date-1)
If my_day < 10 Then 
	my_day =  "0" & my_day
End If

my_year = Year(Date - 1)

//本月&次月最后一天
last_day = Day(DateSerial(Year(Date-1), Month(Date-1) + 1, 1 - 1))
last_day_next = Day(DateSerial(Year(Date-1), Month(Date-1) + 2, 1 - 1))

//次月月份
month_next = Month(Date - 1) + 1
If month_next < 10 Then 
	month_next = "0" & month_next
ElseIf month_next = 13 Then
	month_next = "01"
End If

//是否跳次年
If (Month(Date - 1) + 1) = 13 Then 
	my_year_next = year(Date) + 1

End If

//生成当月自然月rs05日期
//字段一：
my_date_first = my_month & "01" & my_year
TracePrint my_date_first
//字段二：
my_date_last =  my_month & last_day & my_year
TracePrint my_date_last


//生成次月自然月rs05日期
//	字段三：
my_date_first_next = month_next & "01" & my_year
TracePrint my_date_first_next
//	字段四：	
my_date_last_next = month_next & last_day_next & my_year
TracePrint my_date_last_next
'##########日期计算##########

    LockMouse 
    
    Call report_open("rs05")
    
    //专用 - 操作为：报表相关选项选择
	MoveTo 652, 210
	Delay 300
	KeyDown "BackSpace", 1
	Delay 55
	KeyUp "BackSpace", 1

	Delay 300
   	SayString my_date_first
   	Delay 300
   	
	MoveTo 637, 200
	MoveTo 645, 205
	Delay 620
	LeftDown 1
	MoveTo 531, 204
	Delay 504
	LeftUp 1
	Delay 344
	KeyDown "BackSpace", 1
	Delay 88
	KeyUp "BackSpace", 1

    SayString my_date_last
    
    MoveTo 617, 641
    Delay 300
    KeyDown "Tab", 1
    MoveTo 615, 662
    Delay 150
    KeyUp "Tab", 1
    MoveTo 610, 689
    Delay 300
    LeftClick 1
    Delay 470
    KeyDown "LeftAlt", 1
    KeyDown "Alt", 1
    Delay 210
    KeyDown "F", 1
    Delay 100
    KeyUp "F", 1
    Delay 200
    KeyUp "LeftAlt", 1
    KeyUp "Alt", 1
    Delay 1000
    KeyDown "BackSpace", 1
    Delay 83
    KeyUp "BackSpace", 1
	
	Call report_save("RS05-"& Month(Date-1) &"月.pdf")

    UnlockMouse
	
End Function


Function RS05next()
'##########日期计算##########
//夜审前日期确定（非必须）
my_month = Month(Date-1)
If my_month < 10 Then 
	my_month =  "0" & my_month
End If

my_day = Day(Date-1)
If my_day < 10 Then 
	my_day =  "0" & my_day
End If

my_year = Year(Date - 1)

//本月&次月最后一天
last_day = Day(DateSerial(Year(Date-1), Month(Date-1) + 1, 1 - 1))
last_day_next = Day(DateSerial(Year(Date-1), Month(Date-1) + 2, 1 - 1))

//次月月份
month_next = Month(Date - 1) + 1
If month_next < 10 Then 
	month_next = "0" & month_next
ElseIf month_next = 13 Then
	month_next = "01"
End If

//是否跳次年
If (Month(Date - 1) + 1) = 13 Then 
	my_year_next = year(Date) + 1
Else 
	my_year_next = my_year
End If

//生成当月自然月rs05日期
//字段一：
my_date_first = my_month & "01" & my_year
TracePrint my_date_first
//字段二：
my_date_last =  my_month & last_day & my_year
TracePrint my_date_last


//生成次月自然月rs05日期
//	字段三：
my_date_first_next = month_next & "01" & my_year_next
TracePrint my_date_first_next
//	字段四：	
my_date_last_next = month_next & last_day_next & my_year_next
TracePrint my_date_last_next
'##########日期计算##########

    LockMouse 
    
    Call report_open("rs05")
    
    //专用 - 操作为：报表相关选项选择
	MoveTo 652, 210
	Delay 300
	KeyDown "BackSpace", 1
	Delay 55
	KeyUp "BackSpace", 1

	Delay 300
   	SayString my_date_first_next
   	Delay 300
   	
	MoveTo 637, 200
	MoveTo 645, 205
	Delay 620
	LeftDown 1
	MoveTo 531, 204
	Delay 504
	LeftUp 1
	Delay 344
	KeyDown "BackSpace", 1
	Delay 88
	KeyUp "BackSpace", 1

    SayString my_date_last_next
    
    MoveTo 617, 641
    Delay 300
    KeyDown "Tab", 1
    MoveTo 615, 662
    Delay 150
    KeyUp "Tab", 1
    MoveTo 610, 689
    Delay 300
    LeftClick 1
    Delay 470
    KeyDown "LeftAlt", 1
    KeyDown "Alt", 1
    Delay 210
    KeyDown "F", 1
    Delay 100
    KeyUp "F", 1
    Delay 200
    KeyUp "LeftAlt", 1
    KeyUp "Alt", 1
    Delay 1000
    KeyDown "BackSpace", 1
    Delay 83
    KeyUp "BackSpace", 1
	
	Call report_save("RS05-"& month_next &"月.pdf")

    UnlockMouse
	
End Function


